<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--========================== Gray fox ========================-->
	
  <ThingDef ParentName="ThingBaseFox">
    <defName>Fox_Gray</defName>
    <label>gray fox</label>
    <description>Originating from southern half of Earth’s continent of North America. The gray fox is easily distinguishable from the red fox in that they have a silvery gray fur, a black-tipped tail, and reddish fur on its chest and legs.</description>
    <race>
      <meatLabel>fox meat</meatLabel>
	  <wildBiomes>
		<BorealForest>0.001</BorealForest>
		<Tundra>0.001</Tundra>
		<TemperateForest>0.08</TemperateForest>
		<TemperateSwamp>0.08</TemperateSwamp>
	  </wildBiomes>
    </race>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBaseFox">
    <defName>Fox_Gray</defName>
    <label>gray fox</label>
    <labelPlural>gray foxes</labelPlural>
    <race>Fox_Gray</race>
    <lifeStages>
      <li>
        <label>gray fox puppy</label>
        <labelPlural>gray fox puppies</labelPlural>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Fox_Gray</texPath>
          <drawSize>1.0</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Dessicated_Fox_Gray</texPath>
          <drawSize>1.0</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Fox_Gray</texPath>
          <drawSize>1.2</drawSize>
          <shadowData>
            <volume>(0.25, 0.3, 0.25)</volume>
            <offset>(0,0,-0.15)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Dessicated_Fox_Gray</texPath>
          <drawSize>1.2</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Fox_Gray/Fox_Gray</texPath>
          <drawSize>1.68</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Fox_Gray/Dessicated_Fox_Gray</texPath>
          <drawSize>1.68</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>


	<!--========================== Sand fox ========================-->

  <ThingDef ParentName="ThingBaseFox">
    <defName>Fox_Sand</defName>
    <label>sand fox</label>
    <description>Also known as «Rüppell's sand fox» named after Earth’s naturalist Eduard Rüppell. This small fox species lives in the Earth’s North Africa, Middle East and southwest Asia regions.</description>
    <race>
      <useMeatFrom>Fox_Gray</useMeatFrom>
	  <wildBiomes>
		<ExtremeDesert>0.08</ExtremeDesert>
		<Desert>0.1</Desert>
		<AridShrubland>0.15</AridShrubland>
	  </wildBiomes>
    </race>
    <statBases>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
	  <ComfyTemperatureMax>50</ComfyTemperatureMax>
    </statBases>
  </ThingDef>

